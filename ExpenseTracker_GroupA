# lets get started
class itemNode:
    def init (self,data, cost=0):
        self.name = data
        self.cost = cost
        self.category = None


class listNode:

    # initialize
    def init(self):
        self.l = []

    # function to append listNode
    def append(self, Node):
        self.l.append(Node)

    # function to get the total cost of the list
    def getTotal(self, total=0): 
        for each in self.l:
            total += each.cost
        return total
 



